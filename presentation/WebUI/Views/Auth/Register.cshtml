                    <div class="log-in-title">
                        <h3>SaytAdi xoş gəldiniz</h3>
                        <h4>Yeni Hesab Yaradın</h4>
                    </div>

                    <div class="input-box">
                        <form asp-action="Register" class="row g-4">
                            <div class="col-12">
                                <div class="form-floating theme-form-floating">
                                    <input asp-for="UserName" type="text" class="form-control" id="name" placeholder="Ad">
                                    <label for="fullname">Isdifadeci adi</label>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-floating theme-form-floating">
                                    <input asp-for="Name" type="text" class="form-control" id="name" placeholder="Ad">
                                    <label for="fullname">Ad</label>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-floating theme-form-floating">
                                    <input asp-for="Surname" type="text" class="form-control" id="surname" placeholder="Soyad">
                                    <label for="fullname">Soyad</label>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-floating theme-form-floating">
                                    <input asp-for="Email"  type="email" class="form-control" id="email" placeholder="E-poçt ünvanı">
                                    <label for="email">E-poçt ünvanı</label>
                                </div>
                            </div>

                            <div class="col-12">
                                <div class="form-floating theme-form-floating">
                                    <input asp-for="Password" type="password" class="form-control" id="password" placeholder="Şifrə">
                                    <label for="password">Şifrə</label>
                                </div>
                            </div>

                            <div class="col-12">
                                <div class="forgot-box">
                                    <div class="form-check ps-0 m-0 remember-box">
                                        <input class="checkbox_animated check-box" type="checkbox" id="flexCheckDefault">
                                        <label class="form-check-label" for="flexCheckDefault">
                                            Mən razıyam
                                            <a href=""><span>Şərtlər</span></a> və <a href=""><span>Məxfilik</span></a>
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="col-12">
                                <button class="btn btn-animation w-100" type="submit">Qeydiyyatdan Keç</button>
                            </div>
                        </form>
                    </div>

                    <div class="other-log-in">
                        <h6>və ya</h6>
                    </div>

                    <div class="log-in-button">
                        <ul>
                            <li>
                                <a href="https://accounts.google.com/signin/v2/identifier?flowName=GlifWebSignIn&flowEntry=ServiceLogin" class="btn google-button w-100">
                                    <img src="~/assets/images/inner-page/google.png" class="blur-up lazyload" alt=""> Google ilə qeydiyyatdan keçin
                                </a>
                            </li>
                            <li>
                                <a href="https://www.facebook.com/" class="btn google-button w-100">
                                    <img src="~/assets/images/inner-page/facebook.png" class="blur-up lazyload" alt=""> Facebook ilə qeydiyyatdan keçin
                                </a>
                            </li>
                        </ul>
                    </div>

                    <div class="other-log-in">
                        <h6></h6>
                    </div>

                    <div class="sign-up-box">
                        <h4>Artıq bir hesabınız var mı?</h4>
                        <a href="sign_in.html">Daxil ol</a>
                    </div>
                </div>
            </div>

            <div class="col-xxl-7 col-xl-6 col-lg-6"></div>
