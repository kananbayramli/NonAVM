                                </a>
                            </li>
                            <li>
                                <a target="_blank" href="wishlist.html" class="option_links" title="cart">
                                    <lottie-player class="menu--icon" style="width: 2.5rem;" src="https://lottie.host/2d0c20ad-04e7-4710-860a-89a1b18fd19e/yeLfCcYrhN.json" background="transparent" speed="0.5" loop autoplay></lottie-player>
                                </a>
                            </li>
                        </ul>
                    </div>
                </li>
                <li>
                    @await Component.InvokeAsync("UserLayout")
                </li>
                }
