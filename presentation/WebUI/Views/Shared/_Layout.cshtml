    <header class="header clearfix">
        <button type="button" id="toggleMenu" class="toggle_menu">
            <i class='uil uil-bars'></i>
        </button>
        <button id="collapse_menu" class="collapse_menu">
            <i class="uil uil-bars collapse_menu--icon "></i>
            <span class="collapse_menu--label"></span>
        </button>
        <div class="main_logo" id="logo">
            <a asp-controller="Home"><img src="~/assets/images/logo/1.pn" alt="Sayt logo"></a>
            <a asp-controller="Home"><img class="logo-inverse" src="~/assets/images/logo/1.pn" alt="Sayt Adı ct_logo"></a>
        </div>
        <div class="top-category">
            <div class="ui compact menu cate-dpdwn">
                <div class="ui simple dropdown item">
                    <a href="#" class="option_links p-0" title="categories"><i class="uil uil-apps"></i></a>
                    <div class="menu dropdown_category5">
                        <a href="sitemap.html#devicon" class="item channel_item">Geyim</a>
                        <a href="sitemap.html#busicon" class="item channel_item">Ayaqqabı</a>
                        <a target="_blank" href="index-9.html" class="item channel_item">Makiyaj</a>
                        <a href="sitemap.html#iticon" class="item channel_item">İdman Məhsulları</a>
                        <a href="sitemap.html#officon" class="item channel_item">Ofis Ləvazimatları</a>
                        <a href="sitemap.html#pericon" class="item channel_item">Saat</a>
                        <a href="sitemap.html#desicon" class="item channel_item">Aksesuarlar</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="search120">
            <div class="ui search">
                <div class="ui left icon input swdh10">
                    <input class="prompt srch10" type="text" placeholder="Mən axtarıram...">
                    <i class='uil uil-search-alt icon icon1'></i>
                </div>
            </div>
        </div>
        <div class="header_right">
            <ul>
               @if(!User!.Identity!.IsAuthenticated)
                {
                    <li>
                        <a asp-controller="Auth" asp-action="Register" class="btn1542" title="Become an Instructor">Müştəri Qeydiyyatı</a>
                    </li>
                    <li>
                        <a asp-controller="Auth" asp-action="Register" class="btn1542" title="Become an Instructor">Satıcı Qeydiyyatı</a>
                    </li>
                    <li>
                        <a asp-controller="Auth" asp-action="Login" style="background-color: #2e35ef;" class="btn1542" title="Become an Instructor">Daxil ol</a>
                    </li>
                }
                <li>
                    <div class="item_f3">
                        <div class="lng_btn2">
                            <div class="ui language2 bottom right pointing dropdown floating" id="languages" data-content="Select Language">
                                <a href="#"><i class='uil uil-globe lft'></i>Dil<i class='uil uil-angle-down rgt'></i></a>
                                <div class="menu">
                                    <div class="scrolling menu">
                                        <div class="item" data-percent="100" data-value="en" data-english="English">
                                            <span class="description">English</span> English
                                        </div>
                                        <!-- <div class="item" data-percent="37" data-value="ru" data-english="Russian">
                                            <span class="description">Русский</span> Russian
                                        </div> -->
                                        <div class="item" data-percent="11" data-value="tr" data-english="Turkish">
                                            <span class="description">Türkçe</span> Turkish
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
                @if(User!.Identity!.IsAuthenticated){
                <li>
                    <div class="endirim_cart">
                        <ul>
                            <li>
                                <a target="_blank" href="cart.html" class="option_links" title="cart">
